{
  "env": {
      "es6": true,
      "node": true
  },
  "extends": [
      "airbnb-base"
  ],
  "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      },
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "rules": {
    "eslint linebreak-style": 0,
    "no-underscore-dangle": ["error",{"allow":["_id"]}]
  }
}